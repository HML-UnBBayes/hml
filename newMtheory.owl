                                                <isOVariableIn rdf:resource="#Domain_MFrag.land_state_Dry"/>
                                                <isSubsBy rdf:resource="#REGION_label"/>
                                                <rdfs:comment
                                                 rdf:datatype=
                                                "http://www.w3.org/2001/XMLSchema#string"
                                                >land_state_idRegion</rdfs:comment>
                                                <isArgTermIn>
                                                  <SimpleArgRelationship rdf:ID="IX1_1">
                                                    <hasArgNumber
                                                     rdf:datatype=
                                                    "http://www.w3.org/2001/XMLSchema#int"
                                                    >1</hasArgNumber>
                                                    <isArgumentOf rdf:resource="#IX1"/>
                                                    <hasArgTerm rdf:resource="#land_state_Dry.land_state_idRegion"/>
                                                  </SimpleArgRelationship>
                                                </isArgTermIn>
