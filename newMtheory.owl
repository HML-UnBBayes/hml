            <owl:allValuesFrom>
              <owl:Class rdf:ID="Node"/>
            </owl:allValuesFrom>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:about="#isArgumentOf"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Restriction>
            <owl:allValuesFrom>
