                                            <isArgumentOf rdf:resource="#Domain_Res.environmental_factors_Rain"/>
                                          </SimpleArgRelationship>
                                        </isArgTermIn>
                                        <isArgTermIn>
                                          <SimpleArgRelationship rdf:ID="environmental_factors_Humidity_1">
                                            <hasArgTerm rdf:resource="#environmental_factors.environmental_factors_idRegion"/>
                                            <hasArgNumber rdf:datatype=
                                            "http://www.w3.org/2001/XMLSchema#int"
                                            >1</hasArgNumber>
                                            <isArgumentOf rdf:resource="#Domain_Res.environmental_factors_Humidity"/>
                                          </SimpleArgRelationship>
