                                      <DeclarativeDist rdf:ID="plant_PlantType_Table">
                                        <hasDeclaration rdf:datatype=
                                        "http://www.w3.org/2001/XMLSchema#string"
                                        >[GardenPlant = 0.625, Others = 0.375]
			</hasDeclaration>                    <isProbDistOf rdf:resource="#Domain_Res.plant_PlantType"/>
                                      </DeclarativeDist>
                                    </hasProbDist>
                                    <hasPossibleValues>
                                      <CategoricalRVState rdf:ID="Others">
                                        <hasType>
                                          <MetaEntity rdf:ID="CategoryLabel">
                                            <rdfs:comment rdf:datatype=
                                            "http://www.w3.org/2001/XMLSchema#string"
                                            >This MetaEntity should be assigned to the labels for the states of random variables whose domain is a list of categorical values</rdfs:comment>
                                            <isTypeOf>
                                              <CategoricalRVState rdf:ID="Fast">
                                                <hasType rdf:resource="#CategoryLabel"/>
                                                <isPossibleValueOf rdf:resource="#IX1"/>
                                                <isPossibleValueOf>
                                                  <Domain_Res rdf:ID="Domain_Res.environmental_factors_Wind">
                                                    <hasPossibleValues>
                                                      <CategoricalRVState rdf:ID="Slow">
                                                        <hasType rdf:resource="#CategoryLabel"/>
                                                        <isPossibleValueOf rdf:resource="#IX1"/>
                                                        <isPossibleValueOf rdf:resource="#Domain_Res.environmental_factors_Wind"/>
                                                      </CategoricalRVState>
                                                    </hasPossibleValues>
                                                    <hasPossibleValues rdf:resource="#Fast"/>
                                                    <hasInputInstance rdf:resource="#IX1"/>
                                                    <hasArgument>
                                                      <SimpleArgRelationship rdf:ID="environmental_factors_Wind_2">
                                                        <isArgumentOf rdf:resource="#Domain_Res.environmental_factors_Wind"/>
                                                        <hasArgNumber
                                                         rdf:datatype=
                                                        "http://www.w3.org/2001/XMLSchema#int"
                                                        >2</hasArgNumber>
                                                        <hasArgTerm>
                                                          <OVariable rdf:ID="environmental_factors.environmental_factors_idTime">
                                                            <isArgTermIn>
                                                              <SimpleArgRelationship rdf:ID="environmental_factors_Rain_2">
    <hasArgNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
    >2</hasArgNumber>
    <isArgumentOf>
      <Domain_Res rdf:ID="Domain_Res.environmental_factors_Rain">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >environmental_factors_Rain</rdfs:comment>
        <isResidentNodeIn>
          <Domain_MFrag rdf:ID="Domain_MFrag.environmental_factors">
            <isMFragOf rdf:resource="#ROOT_MTheory_1"/>
            <hasResidentNode rdf:resource="#Domain_Res.environmental_factors_Rain"/>
            <hasResidentNode>
              <Domain_Res rdf:ID="Domain_Res.environmental_factors_Humidity">
                <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >environmental_factors_Humidity</rdfs:comment>
                <isResidentNodeIn rdf:resource="#Domain_MFrag.environmental_factors"/>
                <hasPossibleValues>
                  <CategoricalRVState rdf:ID="Low">
                    <hasType rdf:resource="#CategoryLabel"/>
                    <isPossibleValueOf>
                      <Domain_Res rdf:ID="Domain_Res.land_state_Dry">
                        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                        >land_state_Dry</rdfs:comment>
                        <isResidentNodeIn rdf:resource="#Domain_MFrag.land_state_Dry"/>
                        <hasProbDist>
                          <DeclarativeDist rdf:ID="land_state_Dry_Table">
                            <hasDeclaration rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                            >if any land_state_idRegion.land_state_idTime have ( environmental_factors_Wind = Fast ) [High = 0.625, Low = 0.375]
			else if any land_state_idRegion.land_state_idTime have ( environmental_factors_Wind = Slow ) [High = 0.375, Low = 0.625]
			else[ High = 0.5, Low = 0.5]</hasDeclaration>
                            <isProbDistOf rdf:resource="#Domain_Res.land_state_Dry"/>
                          </DeclarativeDist>
                        </hasProbDist>
                        <hasPossibleValues rdf:resource="#Low"/>
                        <hasPossibleValues>
                          <CategoricalRVState rdf:ID="High">
                            <hasType rdf:resource="#CategoryLabel"/>
                            <isPossibleValueOf rdf:resource="#Domain_Res.land_state_Dry"/>
                            <isPossibleValueOf rdf:resource="#Domain_Res.plant_dosage_Dosage"/>
